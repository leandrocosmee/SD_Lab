from typing import Union
class Multiplicar:
    def __init__(self, x:float, y:float):
        """Multiplica 2 numeros e guarda logo o resultado
        Parameters:
            x (float): valor de um numero
            y (float): valor de um numero
        """
        try:
            self.res: float = x * y
        except:
            self.res: str = "Valor inválido"

    def get_resultado(self)->Union[float, str]:
        """Getter do resultado
        #Return: 
            resultado da divisão
        """
        return self.res
    